{
  "extends": [
    "precure/oss",
    "precure/react"
  ],
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/core-modules": [
      "electron",
      "atom"
    ]
  },
  "rules": {
    "no-constant-condition": 0,
    "import/no-extraneous-dependencies": 0,
    "import/exports-last": 0,
    "import/no-unresolved": 0,
    "flowtype/no-weak-types": 0,
    "no-duplicate-imports": 0,
    "no-invalid-this": 0,
  }
}
