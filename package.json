{
  "name": "PixivDeck",
  "productName": "PixivDeck",
  "version": "0.12.1",
  "description": "pixiv client for desktop",
  "keywords": [
    "electron",
    "pixiv"
  ],
  "author": "akameco <akameco.t@gmail.com>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "add": "all-contributors add",
    "generate": "all-contributors generate",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "dev": "cross-env START_HOT=1 npm run hot-updates-server",
    "test": "npm run jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "jest": "cross-env NODE_ENV=test jest",
    "fmt": "prettier --write '{app,config}/**/*.js'",
    "build": "npm run build:main && npm run build:renderer",
    "s2s": "s2s",
    "flow": "flow",
    "i18n": "babel-node ./config/scripts/extract-intl.js",
    "build:dll": "node config/scripts/webpack build-dll",
    "build:main": "node config/scripts/webpack build-main",
    "build:renderer": "node config/scripts/webpack build-renderer",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint \"app/**/*.js\"",
    "lint:js": "eslint .",
    "hot-updates-server": "node config/scripts/webpack hot-server",
    "start-hot-renderer": "node config/scripts/hot-renderer",
    "postinstall": "npm run build:dll && electron-builder install-app-deps",
    "package": "npm run build && build --publish never",
    "release": "npm run build && build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "fmt",
      "git add"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": "stylelint-config-standard",
    "syntax": "scss",
    "rules": {
      "rule-empty-line-before": null,
      "at-rule-empty-line-before": null
    }
  },
  "dependencies": {
    "autolinker": "^1.4.4",
    "axios": "^0.16.2",
    "camelcase-keys": "^4.1.0",
    "decamelize-keys": "^1.1.0",
    "electron-debug": "^1.2.0",
    "keycode": "^2.1.9",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.2",
    "ms": "^2.0.0",
    "normalizr": "^3.2.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-event-listener": "^0.5.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.5",
    "react-sortable-pane": "^0.6.7",
    "redux": "^3.7.1",
    "redux-persist": "^4.10.1",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.1",
    "source-map-support": "^0.4.18",
    "styled-components": "^2.1.1",
    "universal-analytics": "^0.4.13",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.11",
    "@storybook/addon-info": "^3.2.11",
    "@storybook/addon-knobs": "^3.2.10",
    "@storybook/react": "^3.2.11",
    "all-contributors-cli": "^4.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-create-redux-action-func": "^0.1.4",
    "babel-plugin-create-redux-action-type": "^0.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-intl-auto": "^0.10.0",
    "babel-plugin-redux-action-compose": "^0.3.0",
    "babel-plugin-redux-state-compose": "^0.4.0",
    "babel-plugin-s2s-action-creater": "^0.2.6",
    "babel-plugin-s2s-action-root": "^0.3.6",
    "babel-plugin-s2s-action-types": "^0.4.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "boiler-room-custodian": "^0.6.2",
    "chalk": "^2.0.1",
    "concurrently": "^3.5.0",
    "cp-file": "^5.0.0",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.8",
    "electron-builder": "^19.31.1",
    "electron-load-devtool": "^0.5.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "^3.0.1",
    "eslint": "^4.7.2",
    "eslint-config-precure": "^0.8.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "execa": "^0.8.0",
    "extract-react-intl": "^0.3.1",
    "extract-react-intl-messages": "^0.6.0",
    "extract-zip": "^1.6.5",
    "fbjs-scripts": "^0.8.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.58.0",
    "flow-runtime": "^0.14.0",
    "flow-typed": "^2.1.2",
    "husky": "^0.14.3",
    "jest": "^21.2.0",
    "jest-cli": "^21.2.0",
    "jest-styled-components": "^4.6.0",
    "jest-yaml-flat-transfrom": "^0.1.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.0",
    "raf": "^3.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "read-pkg-up": "^2.0.0",
    "s2s": "^0.12.0",
    "s2s-plugins-redux": "^0.3.4",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^1.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0",
    "yaml-flat-loader": "^0.1.0"
  },
  "devEngines": {
    "node": ">=7",
    "npm": ">=3.x",
    "yarn": ">=0.22.x"
  }
}
