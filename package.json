{
  "name": "PixivDeck",
  "productName": "PixivDeck",
  "version": "0.12.1",
  "description": "pixiv client for desktop",
  "keywords": [
    "electron",
    "pixiv"
  ],
  "author": "akameco <akameco.t@gmail.com>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./app/",
    "test": "npm run prettier && npm run jest",
    "jest": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "flow": "flow",
    "fmt": "prettier --write --single-quote --semi=false --jsx-bracket-same-line --trailing-comma es5 --print-width 80 \"{app,config}/**/*.js\"",
    "build": "npm run build-main && npm run build-renderer",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --progress --profile --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --progress --profile --colors",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "hot-updates-server": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "start-hot-renderer": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "dev": "cross-env START_HOT=1 npm run hot-updates-server",
    "flow-typed": "rm -fr flow-typed/npm && flow-typed install --overwrite || true",
    "postinstall": "concurrently \"npm run flow-typed\" \"npm run build-dll\" \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "lint:css": "stylelint \"app/components/**/*.js\"",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl"
  },
  "build": {
    "productName": "PixivDeck",
    "appId": "io.github.akameco.pixivdeck",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "res",
      "output": "release"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": "stylelint-config-standard",
    "syntax": "scss",
    "rules": {
      "indentation": [
        "tab"
      ],
      "rule-empty-line-before": null,
      "at-rule-empty-line-before": null
    }
  },
  "dependencies": {
    "autolinker": "^1.4.3",
    "camelcase-keys": "^4.1.0",
    "electron-debug": "^1.1.0",
    "keycode": "^2.1.8",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "lodash.union": "^4.6.0",
    "lodash.unionby": "^4.8.0",
    "material-ui": "^0.17.4",
    "normalizr": "^3.2.2",
    "pixiv-app-api": "^0.10.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-event-listener": "^0.4.5",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.4",
    "react-sortable-pane": "^0.5.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.8",
    "reselect": "^3.0.0",
    "residual-scroll-top": "^0.1.0",
    "source-map-support": "^0.4.14",
    "styled-components": "^1.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-intl-auto": "^0.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "boiler-room-custodian": "^0.6.2",
    "chalk": "^1.1.3",
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^1.6.6",
    "electron-builder": "^17.1.1",
    "electron-devtools-installer": "^2.2.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-config-xo": "^0.18.1",
    "eslint-config-xo-react": "^0.11.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-xo": "^1.0.0",
    "extract-react-intl": "^0.3.0",
    "extract-react-intl-messages": "^0.3.0",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.44.2",
    "flow-runtime": "^0.10.0",
    "flow-typed": "^2.1.2",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "prettier": "^1.2.2",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.5.4",
    "read-pkg-up": "^2.0.0",
    "redux-logger": "^3.0.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0",
    "yaml-loader": "^0.4.0"
  },
  "devEngines": {
    "node": ">=7",
    "npm": ">=3.x",
    "yarn": ">=0.22.x"
  }
}
